#!/usr/bin/env node

var path = require('path');
var url = require('..').URL;

var resolved = url.fromLocal(path.resolve(process.cwd(), process.argv[2]));

System.config({ baseURL: url.resolve(resolved, '.') });

System.import(
	resolved
).catch(function(err) {
	console.error(err);
	process.exit(1);
});

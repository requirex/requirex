{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Tests",
			"type": "node",
			"request": "launch",
			"args": [
				"--nolazy",
				"-r",
				"ts-node/register/transpile-only",
				"index.ts"
			],
			"sourceMaps": true,
			"cwd": "${workspaceRoot}",
			"preLaunchTask": "npm install",
			"protocol": "inspector"
		}
	]
}
